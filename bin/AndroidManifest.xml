            android:label="@string/how" >
        </activity>
        <activity
            android:name="com.RobinCompany.imageenc.PicSelectActivity"
            android:label="@string/gallerys" >
        </activity>
        <activity
            android:name="com.RobinCompany.imageenc.Decryption"
            android:label="@string/im" >
        </activity>
        <activity
            android:name="com.RobinCompany.imageenc.Splashsc"
            android:label="@string/software" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>